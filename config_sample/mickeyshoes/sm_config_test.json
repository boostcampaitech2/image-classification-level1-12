{
    "name": "team_baseline",
    "n_gpu": 1,

    "model": {
        "type": "MickeyMaskModel",
        "args": {
            "num_classes": 18,
            "version_number" : 18
        }
    },
    "checkpoint_path":  "/opt/ml/team12/image-classification-level1-12/saved/mickeyshoes/models/team_baseline/0902_013221/model_best.pth",
    "dataset": {
        "type": "MaskTrainDataset",
        "args":{
            "root": "/opt/ml/data/train/images",
